{
  "name": "hacker-news-next",
  "version": "0.20.28",
  "description": "used as an example using reSolve with Next.js",
  "private": true,
  "scripts": {
    "build": "next build client && babel-node ./run.js build",
    "cloud": "next build client && babel-node ./run.js cloud",
    "dev": "babel-node ./run.js dev",
    "dev:next": "next dev client",
    "start": "babel-node ./run.js start",
    "test": "jest --testMatch=**/test/unit/*.test.js --verbose",
    "test:functional": "babel-node ./run.js test:functional",
    "import-event-store": "babel-node ./run.js import-event-store",
    "export-event-store": "babel-node ./run.js export-event-store"
  },
  "dependencies": {
    "@babel/node": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "next": "^9.0.7",
    "react": "^16.10.2",
    "react-bootstrap": "0.32.1",
    "react-dom": "^16.10.2",
    "react-helmet": "5.2.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.7",
    "resolve-readmodel-lite": "0.20.28",
    "resolve-readmodel-mysql": "0.20.28",
    "resolve-redux": "0.20.28",
    "resolve-scripts": "0.20.28",
    "resolve-snapshot-mysql": "0.20.28",
    "resolve-storage-lite": "0.20.28",
    "resolve-storage-postgresql-serverless": "0.20.28",
    "resolve-subscribe-mqtt": "0.20.28",
    "resolve-subscribe-socket.io": "0.20.28"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-jest": "24.1.0",
    "fs-extra": "^8.1.0",
    "jest": "24.1.0",
    "react-test-renderer": "16.7.0",
    "resolve-cloud": "*",
    "testcafe": "0.23.3"
  }
}
